{
  "name": "Patent Finder",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Patent Finder",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content_script.js"],
      "all_frames": true
    }
  ],
  "permissions": [],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
